<h1>Welcome</h1>
<p>Below are all of the blogs</p>
<p>Find me in app/views/dashboards/index.html.erb</p>

<table>
    <thead>
        <tr>
        <th><strong>Author</strong></th>
        <th><strong>Blog</strong></th>
        <th colspan="3"><strong>Blog Number</strong></th>

        <th colspan="3"><strong>Action</strong></th>

        </tr>
    </thead>


    <tbody>
        <%= @blog.each do |test| %>
        <tr>
            <td> <%= User.find_by(id: test.user_id).email %>
            <td> <%= test.blog %>
            <td> <%= test.id %>
            


        </tr>
        <% end %>
    </tbody>
</table>

