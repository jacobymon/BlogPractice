<h1>Edit article</h1>


<%= form_with model: Blog, url: dashboards_path do |f| %>
    <p>
      <%= f.label :title %><br/>
      <%= f.text_field :title %>
    </p>
  
    <p>
      <%= f.label :blog %><br/>
      <%= f.text_area :blog %>
    </p>
  
    <p>
      <%= f.submit :"submit" %>
    </p>
<% end %>