<h1>Welcome</h1>
<p>Below are all of the blogs</p>
<p>Find me in app/views/dashboards/index.html.erb</p>
<style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
    }
    th, td {
      padding: 15px;
    }
    </style>
<table>
    <thead>
        <tr>
        <th><strong>Author</strong></th>
        <th><strong>Blog</strong></th>
        <th ><strong>Blog Number</strong></th>
        </tr>
    </thead>

    
    <tbody>
        <% @blog.each do |test| %>
        <tr>
            <td> <%= User.find_by(id: test.user_id).email %>
            <td> <%= test.blog %>
            <td> <%= test.id %>
        </tr>
        <% end %>
    </tbody>
</table>
<%= link_to "Log In", new_login_path%>
